-endif

-declareversions _WIN32_WINNT &h0400 &h0501 &h0502 &h0600 &h0601 &h0602 &h0603 &h0A00
-selectversion
-case &h0400 -define _WIN32_WINNT 0x0400
-case &h0501 -define _WIN32_WINNT 0x0501
